{
  "name": "accessible-design-system",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "yarn workspace @apps/web dev",
    "build": "yarn workspace @apps/web build",
    "storybook": "storybook dev -p 6006 --config-dir .storybook",
    "build-storybook": "storybook build --config-dir .storybook --output-dir apps/web/public/storybook-static"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@netlify/functions": "^2.4.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "9.1.17",
    "@storybook/addon-docs": "9.1.17",
    "@storybook/addon-essentials": "9.1.17",
    "@storybook/addon-interactions": "9.1.17",
    "@storybook/addon-links": "9.1.17",
    "@storybook/blocks": "9.1.17",
    "@storybook/react": "9.1.17",
    "@storybook/react-vite": "9.1.17",
    "@storybook/test": "9.1.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "9.1.17",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

